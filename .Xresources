XTerm.vt100.faceName: Input Mono Compressed:size=14
XTerm.vt100.faceName: Iosevka Term SS15:size=14
XTerm.vt100.faceName: IBM IBM VGA 9x16:style=Regular:size=16

XTerm.vt100.saveLines: 4096
XTerm.vt100.renderFont:true
XTerm*forcePackedFont:true
XTerm.termName: xterm-256color
xterm*bellIsUrgent:true
xterm*background: gray12
xterm*foreground: snow2

Xft.antialias:  true
Xft.hinting:    true
Xft.autohint:   1
Xft.dpi:        96
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb

xterm*VT100.Translations: #override \
                 Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
                 Ctrl Shift <Key>C:    copy-selection(CLIPBOARD)

Xcursor.theme:Adwaita
Xcursor.size: 32
